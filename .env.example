# Server Environment Variables Template
# Copy this file to .env and fill in your actual values

# Required: Get your Gemini API key from: https://aistudio.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# Optional: Port configuration (defaults to 5179)
PORT=5179

# Optional: Environment setting
ENVIRONMENT=development

# Optional: Firebase service account JSON (for production deployment)
# FIREBASE_SERVICE_ACCOUNT={"type":"service_account","project_id":"your-project",...}
